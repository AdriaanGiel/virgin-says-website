.highscore
  height 120vh
  background-color red
  transform-origin left bottom
  //transform skewY(3deg)
  z-index -2
  position relative
  top -100px


.highscore-box
  position relative
  height 75%
  width  75%
  background-color white
  transform skewY(0deg)
  h2
    width 100%
    text-align center
  .highscore-item-content
    width 100%
    height 100%
    highscore-items
      width 75%
      height  75%
